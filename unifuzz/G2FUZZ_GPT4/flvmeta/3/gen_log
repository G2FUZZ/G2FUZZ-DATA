+  # Seek Points (keyframe metadata)
+  seek_point_tag = bytes([
+      0x09,  # Tag type video
+      0x00, 0x00, 0x0A,  # Data size
+      0x00, 0x00, 0x3C, 0x00,  # Timestamp in milliseconds (1000 ms)
+      0x00, 0x00, 0x00,  # StreamID is always 0
+  ])
+  
