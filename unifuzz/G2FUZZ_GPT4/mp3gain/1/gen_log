    os.makedirs('./tmp')

# Generate a 5-second sine wave tone at 440 Hz
tone_duration_ms = 5000  # Duration in milliseconds
frequency_hz = 440  # Frequency in Hertz
sine_wave = Sine(frequency_hz).to_audio_segment(duration=tone_duration_ms)

