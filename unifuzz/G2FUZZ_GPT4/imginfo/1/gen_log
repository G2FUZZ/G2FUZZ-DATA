    # 1-bit monochrome image (black and white)
    image_mono = np.random.randint(2, size=(height, width)) * 255  # 0 or 255
    Image.fromarray(image_mono.astype(np.uint8), 'L').save(f'{directory}/monochrome.ras')

    # 8-bit grayscale image
