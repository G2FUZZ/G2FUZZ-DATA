                elif depth == 8:
                    # Write a black pixel in grayscale
                    file.write(struct.pack('B', 0))
                # Add more depth cases as needed

# Example usage: Create a 100x100 black RAS image
create_ras_file('./tmp/example.ras', 100, 100, 24)
