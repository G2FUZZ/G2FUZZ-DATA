            ExifTags.GPSTAGS[k]: v
            for k, v in {
                0x0001: 'N',  # Latitude reference
                0x0002: ((40, 1), (42, 1), (0, 1)),  # Latitude
                0x0003: 'W',  # Longitude reference
                0x0004: ((74, 1), (0, 1), (0, 1)),  # Longitude
