info.add_text("Interlace", "1")

# Ensure the ./tmp/ directory exists
import os
if not os.path.exists('./tmp/'):
    os.makedirs('./tmp/')

# Save the image with Adam7 interlacing
image.save("./tmp/interlaced_image.png", "PNG", pnginfo=info, interlace=1)
