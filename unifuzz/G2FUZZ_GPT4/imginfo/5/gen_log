```python
import os

# Define the directory and file name
directory = "./tmp"
filename = "graphics_and_images.mif"
file_path = os.path.join(directory, filename)

# Ensure the directory exists
os.makedirs(directory, exist_ok=True)

# Content to be written to the .mif file, focusing on graphics and images feature
content = """
<FrameMaker MIFFile 9.00> # This indicates the MIF file version
# Begin of the document
<AFrames
 <AFRame
  <Unique 1001>
  <FrameType Below>
  <Pen 15>
  <Fill 7>
  <DashedPattern
   <DashedStyle Solid> >
  <PenWidth  0.5>
  <ObColor `Black'>
  <Angle 0>
  <DRect
   <DVal 1.000000>
   <DVal 1.000000>
   <DVal 6.500000>
   <DVal 9.000000> >
  <FrameTag `Graphics and Images'>
  <ImportObject
   <ImportObFile `path/to/your/image.jpg'>
   <ImportObRect
    <DVal 1.000000>
    <DVal 1.000000>
    <DVal 5.000000>
    <DVal 7.000000> > >
  <TextRectID 1002>
  <TextRect
   <TRNum 1002>
   <TRSide Head>
   <TRShape Rect>
   <TROrigin
    <DVal 1.000000>
    <DVal 1.000000> >
   <TRSize
    <DVal 5.000000>
    <DVal 2.000000> >
   <TRNext -1> >
  <GroupID 1003>
  <GroupDisplay Open>
  <BRect
   <BVal 0.000000>
   <BVal 0.000000>
   <BVal 8.000000>
   <BVal 10.000000> > >
# End of the document
<APage
# Page properties and content here
>
"""

# Write the content to the file
with open(file_path, "w") as mif_file:
    mif_file.write(content.strip())

print(f"File '{filename}' successfully created at '{file_path}'.")
