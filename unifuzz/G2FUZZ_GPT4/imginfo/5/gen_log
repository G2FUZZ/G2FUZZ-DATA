def create_bmp_with_rle_compression(filename):
    # BMP Header (14 bytes)
    bmp_header = bytes([
        0x42, 0x4D,  # Signature "BM"
        0x76, 0x00, 0x00, 0x00,  # File size in bytes (118 bytes here)
        0x00, 0x00,  # Reserved
        0x00, 0x00,  # Reserved
