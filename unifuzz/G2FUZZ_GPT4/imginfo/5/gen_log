os.makedirs('./tmp/', exist_ok=True)

# Create both uncompressed and compressed RAS files
create_ras_image('./tmp/uncompressed.ras', 100, 100, compressed=False)
create_ras_image('./tmp/compressed.ras', 100, 100, compressed=True)
