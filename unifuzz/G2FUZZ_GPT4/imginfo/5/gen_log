    os.makedirs(os.path.dirname(filename), exist_ok=True)

    # Write the BMP file
    with open(filename, 'wb') as f:
        f.write(bmp_header)
        f.write(dib_header)
        f.write(color_palette)
        f.write(pixel_data)

