+  # Encrypt the PDF file
+  reader = PdfReader(standard_pdf_path)
+  writer = PdfWriter()
+  for page in reader.pages:
+      writer.add_page(page)
+  writer.encrypt(user_pwd="userpassword", owner_pwd="ownerownerpassword")
