import pytesseract

class PDF(FPDF):
    def header(self):
        self.set_font('Arial', 'B', 12)
        self.cell(0, 10, 'Extended PDF with OCR, Mobile, Print Production, and Object Streams Tools Feature', 0, 1, 'C')

    def footer(self):
