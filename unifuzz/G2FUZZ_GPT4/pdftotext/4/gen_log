    return output_path

# Step 5: Generate PDF, add annotation, apply DRM, and integrate rich media
pdf_path = './tmp/sample.pdf'
annotated_pdf_path = './tmp/sample_annotated.pdf'
drm_pdf_path = './tmp/sample_drm.pdf'
