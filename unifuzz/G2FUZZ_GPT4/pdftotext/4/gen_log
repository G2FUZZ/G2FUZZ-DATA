    for page in range(pdf.getNumPages()):
        # Updated to use PdfWriter
        pdf_writer = PdfWriter()
        pdf_writer.add_page(pdf.getPage(page))
        output_filename = f'{output_folder}/page_{page + 1}.pdf'
