            <dc:format>image/jpeg</dc:format>
            <xmp:CreateDate>2020-01-01T12:00:00</xmp:CreateDate>
            <xmp:ModifyDate>2020-01-02T12:00:00</xmp:ModifyDate>
            <xmp:MetadataDate>2020-01-03T12:00:00</xmp:MetadataDate>
