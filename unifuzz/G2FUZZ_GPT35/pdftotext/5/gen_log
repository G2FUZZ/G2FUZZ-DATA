from fpdf import FPDF

class PDFWithHyperlinks(FPDF):
    def add_link(self, x, y, w, h, link):
        self.link(x, y, w, h, link)

pdf = PDFWithHyperlinks()
pdf.add_page()
pdf.set_font("Arial", size=12)

