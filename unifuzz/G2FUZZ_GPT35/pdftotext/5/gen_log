pdf.set_metadata(author='John Doe', title='Sample PDF with Metadata', keywords='PDF, Metadata, Python', creation_date='2021-09-30')
pdf.add_geospatial_data(location='Coordinates: 40.7128 N, 74.0060 W')

pdf.add_page()
