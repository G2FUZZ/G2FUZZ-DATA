+      header += bytearray([0x41, 0x4E, 0x49, 0x4D])  # "ANIM" in ASCII
+      header += bytearray([0x41, 0x4E, 0x49, 0x20])  # "ANI " in ASCII
+  
+      # Add color depth as 4 bytes little-endian
+      header += color_depth.to_bytes(4, byteorder='little')
