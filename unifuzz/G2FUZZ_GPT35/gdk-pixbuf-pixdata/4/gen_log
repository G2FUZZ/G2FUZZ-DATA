    <xmpRights:Licenses>
      <rdf:Bag>
        <rdf:li>Licenses here</rdf:li>
      </rdf:Bag>
    </xmpRights:Licenses>
    <xmpRights:DigitalRightsManagement>DRM details here</xmpRights:DigitalRightsManagement>
