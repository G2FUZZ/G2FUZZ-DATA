image[:, 50:] = [0, 0, 255]  # Right side of the image in blue

# Applying gamma correction
gamma = 2.2
image_gamma_corrected = (image / 255.0) ** (1 / gamma) * 255
image_gamma_corrected = image_gamma_corrected.astype(np.uint8)

# Saving the image
